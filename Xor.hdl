/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
   Not(in =a, out = nota);
   And (a = nota, b = b, out = notaANDb);
   Not (in = b, out = notb);
   And ( a = notb, b = a, out = notbANDa);
   Or (a = notaANDb, b = notbANDa, out = out);
}